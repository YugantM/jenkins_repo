pipeline{
	agent any 

stages{
	

	stage('clone'){
		steps{

			git url: 'https://github.com/YugantM/PKGBUILD_Tutorial.git'
	}

	stage('Build'){
		steps{
			echo 'desplaying the repo'
			sh 'cd '
			sh 'git clone https://github.com/YugantM/PKGBUILD_Tutorial.git'
			sh 'cd /add_PKGBUILD'
			sh 'makepkg'
	}

}

}

}